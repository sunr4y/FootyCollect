[run]
source = collection/repositories
omit =
    */migrations/*
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    *.md
    *.gif
    *.png
    *.jpg
    *.jpeg
    *.html
    *.css
    *.js
    *.json
    *.yaml
    *.yml
    *.toml

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
